{
    "name": "nozavroni/mainframe-router",
    "description": "A router component for Mainframe",
    "type": "library",
    "authors": [
        {
            "name": "Luke Visinoni",
            "email": "luke.visinoni@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4.1"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "doctrine/annotations": "^1.10",
        "monolog/monolog": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/options-resolver": "^5.0",
        "symfony/string": "^5.0",
        "symfony/var-dumper": "^5.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Mainframe\\Utils\\": "./src/"
        },
        "files": [
            "./src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MainframeTest\\Utils\\": "./tests/"
        }
    }
}
