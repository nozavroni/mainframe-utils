{
    "name": "nozavroni/mainframe-utils",
    "description": "Utilities (various classes and functions) in support of the Mainframe (both its core and its components)",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Luke Visinoni",
            "email": "luke.visinoni@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4.1"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "symfony/options-resolver": "^5.0",
        "symfony/string": "^5.0",
        "symfony/var-dumper": "^5.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Mainframe\\Utils\\": "./src/"
        },
        "files": [
            "./src/functions.php",
            "./src/nsfunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MainframeTest\\Utils\\": "./tests/"
        }
    }
}
